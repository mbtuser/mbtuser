<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name = "aob">
    <parameter name = "firstName" value="Sample first Name"/>
    <parameter name = "lastName" value="Sample last Name"/>
    <parameter name = "email" value="samples@opentext.com"/>
    <parameter name = "userName" value="Samples"/>
    <parameter name = "password" value="Aa1234567890"/>
    <parameter name = "confirmPassword" value="Aa1234567890"/>
    <test name = "RegisterNewUser">
        <classes>
            <class name = "opentext.mbt.tests.Samples" >
                <methods>
                    <include name="RegisterNewUser" />
                </methods>
            </class>
        </classes>
    </test>
    <parameter name = "userName" value="admin"/>
    <parameter name = "password" value="demo"/>
    <test name = "Login">
        <classes>
            <class name = "opentext.mbt.tests.Samples" >
                <methods>
                    <include name="Login" />
                </methods>
            </class>
        </classes>
    </test>
    <parameter name = "accountName" value="Sample Account"/>
    <test name = "OpenNewAccount">
        <classes>
            <class name = "opentext.mbt.tests.Samples" >
                <methods>
                    <include name="OpenNewAccount" />
                </methods>
            </class>
        </classes>
    </test>
</suite>